{
	"root": "/root/test",
	"exported_tags": [{
		"name": "branch",
		"prefix": "/",
		"default": "curr"
	}, {
		"name": "platform",
		"prefix": "/${branch}/ds.",
		"default": "base"
	}],
	"hidden_tags": [{
		"name": "project",
		"prefix": "/${branch}/ds.${platform}/source/",
		"condition": "[ \"packages\" != \"${branch}\" ]"
	}, {
		"name": "package",
		"prefix": "/packages/source/",
		"condition": "[ \"packages\" = \"${branch}\" ]"
	}],
	"bookmarks": [{
		"prefix": "/${branch}/ds.${platform}/source/",
		"condition": "[ \"packages\" != \"${branch}\" ]",
		"items": {
			"${project}": "${project}",
			"src": "",
			"ls": "../lnxscripts"
		}
	}, {
		"prefix": "/packages/source",
		"condition": "[ \"packages\" = \"${branch}\" ]",
		"items": {
			"${package}": "${package}",
			"src": "",
			"ls": "../pkgscript-ng"
		}
	}]
}
